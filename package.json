{
  "name": "inventas-app",
  "version": "1.0.0",
  "description": "Inventas-App",
  "author": "Ernesto DÃ­az",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "start": "pm2 start dist/src/main.js --name inventas-app-backend",
    "kill": "pm2 kill",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --watch --debug",
    "seed:create": "typeorm migration:create database/migrations/seed",
    "migration:create": "typeorm migration:create database/migrations/mig",
    "typeorm": "npm run build && npx typeorm -d dist/database/database.config.js",
    "database:sync": "npm run typeorm -- migration:run",
    "serverless:local": "serverless offline --stage local",
    "deploy-serverless:develop": "serverless deploy --aws-profile personal --stage develop",
    "deploy-serverless:qa": "serverless deploy --aws-profile personal --stage qa",
    "deploy-serverless:production": "serverless deploy --aws-profile personal --stage production",
    "undeploy-serverless:develop": "serverless remove --aws-profile personal --stage develop",
    "undeploy-serverless:qa": "serverless remove --aws-profile personal --stage qa",
    "undeploy-serverless:production": "serverless remove --aws-profile personal --stage production",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,md}\""
  },
  "dependencies": {
    "@nestjs/cache-manager": "^2.2.2",
    "@nestjs/common": "^10.3.10",
    "@nestjs/config": "^3.2.3",
    "@nestjs/core": "^10.3.10",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/mapped-types": "*",
    "@nestjs/microservices": "^10.3.10",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.3.10",
    "@nestjs/swagger": "^7.4.0",
    "@nestjs/typeorm": "^10.0.2",
    "@vendia/serverless-express": "^4.12.6",
    "aws-lambda": "^1.0.7",
    "axios": "^1.7.4",
    "bcryptjs": "^2.4.3",
    "cache-manager": "^5.7.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cross-env": "^7.0.3",
    "crypto-js": "^4.2.0",
    "dayjs": "^1.11.12",
    "firebase": "^10.12.4",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "nestjs-typeorm-paginate": "^4.0.4",
    "nodemailer": "^6.9.14",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.12.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "typeorm": "0.3.20",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.4.2",
    "@nestjs/schematics": "^10.1.3",
    "@nestjs/testing": "^10.3.10",
    "@types/aws-lambda": "^8.10.143",
    "@types/crypto-js": "^4.2.2",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.12",
    "@types/nodemailer": "^6.4.15",
    "@types/passport": "^1.0.16",
    "@types/passport-jwt": "^4.0.1",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^7.17.0",
    "@typescript-eslint/parser": "^7.17.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "29.7.0",
    "prettier": "^3.3.3",
    "serverless": "~3.39.0",
    "serverless-jetpack": "^0.11.2",
    "serverless-offline": "^13.6.0",
    "serverless-plugin-optimize": "^4.2.1-rc.1",
    "serverless-plugin-typescript": "^2.1.5",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "29.2.3",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "^5.5.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "coveragePathIgnorePatterns": [
      "main.ts",
      "\\.dto\\.ts$",
      "\\.exception\\.ts$",
      "\\.strategy\\.ts$",
      "\\.config\\.ts$",
      "\\.module\\.ts$"
    ],
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/apps/"
    ]
  }
}
